
@{
    ViewBag.Title = "KullaniciKayit";
    Layout = "~/Views/Shared/_KullaniciLayout.cshtml";
}

<div class="row login-css">
    <div class="login col-xs-4">
        <form action="/action_page.php">
            <div class="form-group login-input">
                <input type="email" class="form-control" placeholder="Kullanıcı Ad" name="kullaniciAd">
            </div>

            <div class="form-group login-input">
                <input type="email" class="form-control" placeholder="Kullanıcı Soyad" name="kullaniciSoyad">
            </div>

            <div class="form-group login-input">
                <input type="email" class="form-control" placeholder="Kullanıcı Mail" name="kullaniciMail">
            </div>

            <div class="form-group login-input">
                <input type="email" class="form-control" placeholder="Kullanıcı Telefon" name="kullaniciTel">
            </div>

            <div class="form-group login-input">
                <input type="email" class="form-control" placeholder="Kullanıcı TC" name="kullaniciTC">
            </div>

            <div class="form-group login-input">
                <input type="email" class="form-control" placeholder="Doğum Tarihi GG/AA/YY" name="kullaniciDogumTarihi">
            </div>

            <div class="form-group  login-input">
                <input type="password" class="form-control" placeholder="Enter password" name="kullaniciSifre">
            </div>
            <button type="submit" class="btn btn-primary kayit">Üye Ol</button>
